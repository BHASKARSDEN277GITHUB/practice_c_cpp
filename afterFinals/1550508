<script type="text/javascript">
    $('a, button, input').live('click', function(e) {
        src = $(e.currentTarget);
        if (src.attr('data-analytics')) {
            action = 'Click'; data = src.attr('data-analytics');
        } else {
            return;
        }

        // mixpanel
        mixpanel.push(['track', action, {
            'UserName' : '',
            'Data' : data, 'Page' : '/submissions/code/1550508',
            'PageType' : 'HomePage',
            'Domain': 'www.hackerrank.com'
        }]);

        // google analytics
        _gaq.push(['_trackEvent', 'Events' , action, data])
    });
</script>

